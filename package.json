{
  "name": "@paulincai/mup-upd",
  "version": "2.0.2",
  "description": "Production Quality Meteor Deployments",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulincai/mup-upd.git"
  },
  "keywords": [
    "meteor",
    "meteor-up",
    "deploy"
  ],
  "author": "Kadira Inc.",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/zodern/meteor-up/issues"
  },
  "homepage": "http://meteor-up.com/",
  "bin": {
    "mup": "./lib/index.js"
  },
  "scripts": {
    "prepare": "npm run build -s",
    "build": "babel src --out-dir lib --copy-files --source-maps",
    "build:watch": "npm run build -s -- -w",
    "test": "node ./tests/run.js",
    "test:module": "mocha --require @babel/register",
    "test:parallel": "bash ./tests/run-parallel.sh",
    "test:unit": "cross-env NODE_ENV=test nyc --all mocha --require @babel/register src/**/*.unit.js",
    "lint": "npm run lint:code && npm run lint:defaultConfig && npm run lint:docs-configs",
    "lint:defaultConfig": "eslint **/*.sample --rule=\"comma-dangle: 0\"",
    "lint:code": "eslint .",
    "lint:docs-configs": "node tests/validate-docs.js",
    "docs": "cd docs && bundle exec jekyll serve",
    "postinstall": "opencollective-postinstall || (exit 0)",
    "clean-shrinkwrap": "cd tests && node clean-shrinkwrap.js"
  },
  "dependencies": {
    "@zodern/nodemiral": "^2.0.0-beta.1",
    "axios": "^1.13.5",
    "bluebird": "^3.7.2",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "debug": "^4.4.3",
    "expand-tilde": "^2.0.2",
    "global-modules": "^2.0.0",
    "joi": "^18.0.2",
    "lodash": "^4.17.23",
    "opencollective-postinstall": "^2.0.3",
    "parse-json": "^8.3.0",
    "random-seed": "0.3.0",
    "resolve-from": "^5.0.0",
    "rimraf": "^6.1.2",
    "shell-escape": "^0.2.0",
    "shelljs": "^0.10.0",
    "ssh2": "^1.16.0",
    "tar": "^7.5.7",
    "traverse": "^0.6.11",
    "uuid": "^13.0.0",
    "www-authenticate": "^0.6.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-class-properties": "^7.28.6",
    "@babel/plugin-transform-object-rest-spread": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@babel/register": "^7.28.6",
    "babel-plugin-istanbul": "^7.0.1",
    "browserslist": "^4.28.1",
    "chai": "^6.2.2",
    "chai-string": "^2.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^10.0.0",
    "gfm-code-blocks": "^1.0.0",
    "globals": "^17.3.0",
    "keypair": "^1.0.4",
    "mocha": "^11.7.5",
    "node-forge": "^1.3.3",
    "nyc": "^17.1.0",
    "sinon": "^21.0.1"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "src/plugins/**",
      "**/__tests__/**"
    ]
  },
  "collective": {
    "url": "https://opencollective.com/meteor-up"
  },
  "engines": {
    "node": ">=22.20.0",
    "npm": ">=10.5.0"
  }
}